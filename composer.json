{
    "name": "vikingmaster/tampere-journeys-api-sdk",
    "description": "PHP SDK for Tampere Journeys API",
    "version": "0.1.2",
    "type": "library",
    "license": "MIT",
    "keywords": ["tampere", "journeys", "api", "php", "sdk", "smart-city", "bus"],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*",
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
